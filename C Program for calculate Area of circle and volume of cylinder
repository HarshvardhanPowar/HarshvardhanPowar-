// C Progarm to calculate area of cirle and modify the program to calculate the volume of cylinder given radius and height

#include<stdio.h>
void main()
{
int radius=3;

float pi=3.14;

printf("The Radius of circle is %f",pi*radius*radius);

int height=3;

printf("Volume of this cylinder is %f",pi*radius*radius*height);

return0;
}
